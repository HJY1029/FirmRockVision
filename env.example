# 磐石之眼（FirmRock Vision）- 环境变量配置示例
# 复制此文件为 .env 并填入实际值

# Flask配置
FLASK_ENV=production
FLASK_DEBUG=False
SECRET_KEY=your-secret-key-change-this-in-production

# 服务器配置
PORT=5000
HOST=0.0.0.0

# 数据库配置（如果使用外部数据库）
# DATABASE_URL=sqlite:///detection.db

# 文件上传配置
MAX_CONTENT_LENGTH=209715200  # 200MB in bytes
UPLOAD_FOLDER=uploads

# 请求超时（秒）
REQUEST_TIMEOUT=600

